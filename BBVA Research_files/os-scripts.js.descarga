function isItemInArray(id, arr) {
    for (var i = 0; i < arr.length; i++) {
        if (arr[i][0] == id) {
            return true; // Found it
        }
    }
    return false; // Not found
}

function deleteItemInArray(id, arr) {
    for (var i = 0; i < arr.length; i++) {
        if (arr[i][0] == id) {
            arr.splice(i, 1);
            return;
        }
    }
}

